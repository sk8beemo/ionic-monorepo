    "@angular/platform-browser": "^21.1.1",
    "@angular/router": "^21.1.1",
    "@capacitor/core": "^7.0.0",
    "@ionic/angular": "^8.7.17",
    "@jsverse/transloco": "^7.0.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  }
}
