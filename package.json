{
  "name": "@ionic-monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx serve ionic-app",
    "build": "nx build ionic-app",
    "build:prod": "nx build ionic-app --configuration=production",
    "build:ios": "pnpm build:prod && nx run ionic-app:cap --cmd=\"sync ios\"",
    "build:android": "pnpm build:prod && nx run ionic-app:cap --cmd=\"sync android\"",
    "gen": "nx generate @nxext/capacitor:configuration --project ionic-app",
    "test": "nx test ionic-app",
    "lint": "nx lint ionic-app",
    "graph": "nx graph",
    "cap:add:ios": "nx run ionic-app:cap --cmd=\"add ios\"",
    "cap:add:android": "nx run ionic-app:cap --cmd=\"add android\"",
    "cap:sync": "pnpm build && nx run ionic-app:cap --cmd=\"sync\"",
    "cap:sync:ios": "pnpm build && nx run ionic-app:cap --cmd=\"sync ios\"",
    "cap:sync:android": "pnpm build && nx run ionic-app:cap --cmd=\"sync android\"",
    "cap:open:ios": "nx run ionic-app:cap --cmd=\"open ios\"",
    "cap:open:android": "nx run ionic-app:cap --cmd=\"open android\"",
    "run:ios": "pnpm build:prod && nx run ionic-app:cap --cmd=\"sync ios\" && nx run ionic-app:cap --cmd=\"run ios\"",
    "run:android": "pnpm build:prod && nx run ionic-app:cap --cmd=\"sync android\" && nx run ionic-app:cap --cmd=\"run android\""
  },
  "private": true,
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "~2.1.2",
    "@analogjs/vitest-angular": "~2.1.2",
    "@angular-devkit/build-angular": "^21.1.1",
    "@angular-devkit/core": "^21.1.1",
    "@angular-devkit/schematics": "^21.1.1",
    "@angular/build": "^21.1.1",
    "@capacitor/android": "^7.0.0",
    "@capacitor/cli": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "22.4.1",
    "@nx/devkit": "22.4.1",
    "@nx/eslint": "22.4.1",
    "@nx/eslint-plugin": "22.4.1",
    "@nx/js": "22.4.1",
    "@nx/playwright": "22.4.1",
    "@nx/vite": "22.4.1",
    "@nx/vitest": "22.4.1",
    "@nx/web": "22.4.1",
    "@nx/workspace": "22.4.1",
    "@nxext/capacitor": "^21.0.0",
    "@nxext/ionic-angular": "^21.0.0",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "^21.1.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "typed-assert": "^1.0.9",
    "@typescript-eslint/utils": "^8.40.0",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.8",
    "angular-eslint": "^21.0.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-playwright": "^1.6.2",
    "jsdom": "^27.1.0",
    "nx": "^22.4.1",
    "prettier": "~3.6.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.0.0",
    "vitest": "^4.0.8"
  },
  "dependencies": {
